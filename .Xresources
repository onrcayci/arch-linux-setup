! rxvt-unicode terminal configuration
URxvt.font: xft:Fira Code:size=10,xft:monospace:size=10
URxvt.scrollstyle: plain
! Disable scroll bar from appearing on the window
URxvt.scrollBar: false
! Adjust letter spacing to be closer than default
URxvt.letterSpace: -1

! Dracula color scheme
*.foreground: #F8F8F2
*.background: #282A36
*.color0: #000000
*.color8: #4D4D4D
*.color1: #FF5555
*.color9: #FF6E67
*.color2: #50FA7B
*.color10: #5AF78E
*.color3: #F1FA8C
*.color11: #F4F99D
*.color4: #BD93F9
*.color12: #CAA9FA
*.color5: #FF79C6
*.color13: #FF92D0
*.color6: #8BE9FD
*.color14: #9AEDFE
*.color7: #BFBFBF
*.color15: #E6E6E6
